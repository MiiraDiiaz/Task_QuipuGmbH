<Window x:Class="Task_QuipuGmbH.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task_QuipuGmbH"
        mc:Ignorable="d"
        Title="MainWindow" Height="280.048" Width="552.9">
    <Grid Margin="10,10,33.667,40.333" RenderTransformOrigin="0.746,0.701">
        <Button x:Name="_startButton"
              Width="87"
              Margin="10,0,0,153"
              HorizontalAlignment="Left"
              VerticalAlignment="Bottom"
              Click="Button_Start"
              Content="Начать" RenderTransformOrigin="0.52,-0.408" Height="20" />

        <DataGrid x:Name="GridUrl"
                Margin="0,69,25,-0.666" Grid.ColumnSpan="2" >
        </DataGrid>

        <TextBlock x:Name="_infoBlock"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    TextWrapping="Wrap" Height="15" RenderTransformOrigin="0.525,1.85" Margin="0,54,187.667,0" Width="155" Grid.ColumnSpan="2" />
        <ProgressBar x:Name="_progressBar"
                      Height="20"
                      Margin="174,34,46.999,0"
                      VerticalAlignment="Top" RenderTransformOrigin="0.846,5.227" Grid.ColumnSpan="2" />
    </Grid>
</Window>
